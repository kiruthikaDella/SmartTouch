<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_accountSettingsFragment"
            app:destination="@id/accountSettingsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_faqsFragment"
            app:destination="@id/faqsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_roomPanelFragment"
            app:destination="@id/roomPanelFragment" />
    </fragment>
    <action
        android:id="@+id/action_global_homeFragment"
        app:destination="@id/homeFragment" />
    <fragment
        android:id="@+id/roomPanelFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.DeviceFragment"
        android:label="fragment_room_panel"
        tools:layout="@layout/fragment_device">
        <argument
            android:name="roomDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetRoomData" />
        <action
            android:id="@+id/action_roomPanelFragment_to_deviceCustomizationFragment"
            app:destination="@id/deviceCustomizationFragment" />
        <action
            android:id="@+id/action_roomPanelFragment_to_deviceFeaturesFragment"
            app:destination="@id/deviceFeaturesFragment" />
        <action
            android:id="@+id/action_roomPanelFragment_to_deviceSettingsFragment"
            app:destination="@id/deviceSettingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/controlModeFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.controlmode.ControlModeFragment"
        android:label="fragment_control_mode"
        tools:layout="@layout/fragment_control_mode" />
    <fragment
        android:id="@+id/deviceCustomizationFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.DeviceCustomizationFragment"
        android:label="fragment_device_customization"
        tools:layout="@layout/fragment_device_customization" >
        <action
            android:id="@+id/action_deviceCustomizationFragment_to_switchIconsFragment"
            app:destination="@id/switchIconsFragment" />
        <argument
            android:name="deviceDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetDeviceData" />
        <argument
            android:name="roomDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetRoomData" />
        <action
            android:id="@+id/action_deviceCustomizationFragment_to_screenLayoutFragment"
            app:destination="@id/screenLayoutFragment" />
    </fragment>
    <dialog
        android:id="@+id/screenLayoutFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.ScreenLayoutFragment"
        android:label="fragment_screen_layout"
        tools:layout="@layout/fragment_screen_layout" >
        <argument
            android:name="deviceCustomizationDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.DeviceCustomizationData" />
        <argument
            android:name="deviceDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetDeviceData" />
        <argument
            android:name="roomDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetRoomData" />
        <action
            android:id="@+id/action_screenLayoutFragment_to_roomPanelFragment"
            app:destination="@id/roomPanelFragment"
            app:popUpTo="@id/roomPanelFragment"
            app:popUpToInclusive="true" />
    </dialog>
    <fragment
        android:id="@+id/deviceFeaturesFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.DeviceFeaturesFragment"
        android:label="fragment_device_features"
        tools:layout="@layout/fragment_device_features" >
        <argument
            android:name="deviceDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetDeviceData" />
        <action
            android:id="@+id/action_deviceFeaturesFragment_to_sceneFragment"
            app:destination="@id/sceneFragment" />
        <argument
            android:name="roomDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetRoomData" />
    </fragment>
    <fragment
        android:id="@+id/createSceneFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.CreateSceneFragment"
        android:label="fragment_create_scene"
        tools:layout="@layout/fragment_create_scene" >
        <argument
            android:name="deviceDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetDeviceData" />
        <argument
            android:name="roomDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetRoomData" />
        <argument
            android:name="sceneDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetSceneData"
            app:nullable="true" />
        <argument
            android:name="controlModeList"
            app:argType="com.dellainfotech.smartTouch.api.model.ControlModeRoomData[]" />
        <action
            android:id="@+id/action_createSceneFragment_to_roomPanelFragment"
            app:destination="@id/roomPanelFragment"
            app:popUpTo="@id/roomPanelFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/accountSettingsFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.AccountSettingsFragment"
        android:label="fragment_account_settings"
        tools:layout="@layout/fragment_account_settings" />
    <fragment
        android:id="@+id/faqsFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.FaqsFragment"
        android:label="fragment_faqs"
        tools:layout="@layout/fragment_faqs" />
    <fragment
        android:id="@+id/contactUsFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.contact.ContactUsFragment"
        android:label="fragment_contact_us"
        tools:layout="@layout/fragment_contact_us" />
    <fragment
        android:id="@+id/switchIconsFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.SwitchIconsFragment"
        android:label="fragment_switch_icons"
        tools:layout="@layout/fragment_switch_icons" >
        <action
            android:id="@+id/action_switchIconsFragment_to_switchIconsDetailFragment"
            app:destination="@id/switchIconsDetailFragment" />
        <argument
            android:name="deviceDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetDeviceData" />
        <argument
            android:name="roomDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetRoomData" />
        <action
            android:id="@+id/action_switchIconsFragment_to_roomPanelFragment"
            app:destination="@id/roomPanelFragment"
            app:popUpTo="@id/roomPanelFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/switchIconsDetailFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.SwitchIconsDetailFragment"
        android:label="fragment_switch_icons_detail"
        tools:layout="@layout/fragment_switch_icons_detail" >
        <argument
            android:name="switchDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.DeviceSwitchData" />
        <argument
            android:name="roomDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetRoomData" />
        <action
            android:id="@+id/action_switchIconsDetailFragment_to_roomPanelFragment"
            app:destination="@id/roomPanelFragment"
            app:popUpTo="@id/roomPanelFragment"
            app:popUpToInclusive="true" />
        <argument android:name="deviceDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetDeviceData" />
    </fragment>
    <fragment
        android:id="@+id/userManagementFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.usermanagement.UserManagementFragment"
        android:label="fragment_user_management"
        tools:layout="@layout/fragment_user_management" >
        <action
            android:id="@+id/action_userManagementFragment_to_addUserFragment"
            app:destination="@id/addUserFragment" />
    </fragment>
    <fragment
        android:id="@+id/addUserFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.usermanagement.AddUserFragment"
        android:label="fragment_add_user"
        tools:layout="@layout/fragment_add_user" />
    <fragment
        android:id="@+id/deviceSettingsFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.DeviceSettingsFragment"
        android:label="fragment_device_settings"
        tools:layout="@layout/fragment_device_settings" >
        <argument
            android:name="deviceDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetDeviceData" />
    </fragment><action android:id="@+id/action_global_userManagementFragment" app:destination="@id/userManagementFragment"/><action android:id="@+id/action_global_contactUsFragment" app:destination="@id/contactUsFragment"/><action android:id="@+id/action_global_controlModeFragment" app:destination="@id/controlModeFragment"/>
    <fragment
        android:id="@+id/sceneFragment"
        android:name="com.dellainfotech.smartTouch.ui.fragments.main.home.SceneFragment"
        android:label="fragment_scene"
        tools:layout="@layout/fragment_scene" >
        <action
            android:id="@+id/action_sceneFragment_to_createSceneFragment"
            app:destination="@id/createSceneFragment" />
        <argument
            android:name="deviceDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetDeviceData" />
        <argument
            android:name="roomDetail"
            app:argType="com.dellainfotech.smartTouch.api.model.GetRoomData" />
        <action
            android:id="@+id/action_sceneFragment_to_roomPanelFragment"
            app:destination="@id/roomPanelFragment"
            app:popUpTo="@id/roomPanelFragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>