                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_5sdp"
                android:textSize="@dimen/_11ssp"
                android:fontFamily="@font/roboto_medium"
                android:textColor="@color/theme_color"
                android:maxLength="8"
                android:singleLine="true"
                app:layout_constraintStart_toStartOf="@+id/guideline2"
                app:layout_constraintTop_toTopOf="@+id/tv_switch_one_name_bottom"
                app:layout_constraintBottom_toBottomOf="@+id/tv_switch_one_name_bottom"
                app:layout_constraintEnd_toEndOf="@+id/switch_one_bottom" />

            <com.google.android.material.switchmaterial.SwitchMaterial
                android:id="@+id/switch_one_bottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/PanelSwitchColorStyle"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/switch_usb_port_c" />

            <!--        Switch two        -->

            <ImageView
                android:id="@+id/iv_switch_two_network_bottom"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_gravity="center"
                android:src="@drawable/ic_switch_network"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/switch_two_bottom"
                app:layout_constraintBottom_toBottomOf="@+id/switch_two_bottom" />

            <TextView
                android:id="@+id/tv_switch_two_name_bottom"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/text_switch_two"
                android:textSize="@dimen/_12ssp"
                android:fontFamily="@font/roboto_medium"
                android:textColor="@color/daintree"
                android:maxLength="8"
                android:singleLine="true"
                android:layout_marginStart="@dimen/_5sdp"
                app:layout_constraintStart_toEndOf="@+id/iv_switch_two_network_bottom"
                app:layout_constraintTop_toTopOf="@+id/iv_switch_two_network_bottom"
                app:layout_constraintBottom_toBottomOf="@+id/iv_switch_two_network_bottom"
                app:layout_constraintEnd_toEndOf="@+id/guideline1" />

            <TextView
                android:id="@+id/tv_switch_two_type_bottom"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:textSize="@dimen/_11ssp"
