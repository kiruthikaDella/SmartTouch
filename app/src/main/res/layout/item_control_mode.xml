            <com.google.android.material.switchmaterial.SwitchMaterial
                android:id="@+id/switch_usb_port_c"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/PanelSwitchColorStyle"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/seek_bar" />

            <!--        Switch one        -->

            <ImageView
                android:id="@+id/iv_switch_one_network_bottom"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_gravity="center"
                android:src="@drawable/ic_switch_network"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/switch_one_bottom"
                app:layout_constraintBottom_toBottomOf="@+id/switch_one_bottom" />

            <TextView
