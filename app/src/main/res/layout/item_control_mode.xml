                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/text_switch_one"
                android:textSize="@dimen/_12ssp"
                android:fontFamily="@font/roboto_medium"
                android:textColor="@color/daintree"
                android:maxLength="8"
                android:singleLine="true"
                android:layout_marginStart="@dimen/_5sdp"
                app:layout_constraintStart_toEndOf="@+id/iv_switch_one_network_bottom"
                app:layout_constraintTop_toTopOf="@+id/iv_switch_one_network_bottom"
                app:layout_constraintBottom_toBottomOf="@+id/iv_switch_one_network_bottom"
                app:layout_constraintEnd_toEndOf="@+id/guideline1" />

            <TextView
                android:id="@+id/tv_switch_one_type_bottom"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:textSize="@dimen/_11ssp"
                android:fontFamily="@font/roboto_medium"
                android:textColor="@color/gull_gray"
                android:maxLength="8"
                android:singleLine="true"
                app:layout_constraintStart_toStartOf="@+id/guideline1"
                app:layout_constraintEnd_toEndOf="@+id/guideline2"
                app:layout_constraintTop_toTopOf="@+id/tv_switch_one_name_bottom"
                app:layout_constraintBottom_toBottomOf="@+id/tv_switch_one_name_bottom" />

            <TextView
                android:id="@+id/tv_switch_one_edit_bottom"
